{
    "name": "tourad/laravel-user-manager",
    "description": "A comprehensive Laravel package for advanced user management with roles, permissions, user types, session tracking, activity logging, and security features",
    "version": "2.0.0",
    "type": "library",
    "keywords": ["laravel", "user-management", "roles", "permissions", "authentication", "user-types", "session-tracking", "activity-logging", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tourad",
            "email": "touradmedlemin17734@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/auth": "^8.0|^9.0|^10.0|^11.0",
        "spatie/laravel-permission": "^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tourad\\UserManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tourad\\UserManager\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tourad\\UserManager\\UserManagerServiceProvider"
            ],
            "aliases": {
                "UserManager": "Tourad\\UserManager\\Facades\\UserManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}